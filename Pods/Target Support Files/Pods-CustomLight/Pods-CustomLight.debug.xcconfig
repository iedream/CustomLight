GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MultiSelectSegmentedControl" "${PODS_ROOT}/Headers/Public/iOS-color-wheel"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/MultiSelectSegmentedControl" "$PODS_CONFIGURATION_BUILD_DIR/iOS-color-wheel"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MultiSelectSegmentedControl" -isystem "${PODS_ROOT}/Headers/Public/iOS-color-wheel"
OTHER_LDFLAGS = $(inherited) -ObjC -l"MultiSelectSegmentedControl" -l"iOS-color-wheel" -framework "CoreGraphics" -framework "Foundation" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
